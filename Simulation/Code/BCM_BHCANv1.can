{
//  current_ign = sysGetVariableInt(sysvar::RoadMap::OperationalModeSts);
//  if((previous_ign == (0x0 || 0x1 || 0x2 || 0x3)) && (current_ign == (0x4 || 0x5 || 0x6 || 0x7 || 0x8)))

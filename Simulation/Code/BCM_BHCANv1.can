on Timer stuckButton
{
  switch(button)
  {
    case up:
      SWC_msg.Command_12Sts = 0x01;
    break;
    
    case down:
      SWC_msg.Command_13Sts = 0x01;
    break;
      
    case left:
      SWC_msg.Command_15Sts = 0x01;
    break;
      
    case right:
      SWC_msg.Command_14Sts = 0x01;
    break;
      
    case enter:
      SWC_msg.Command_11Sts = 0x01;
    break;      
     
    case home:
      SWC_msg.Command_10Sts = 0x01;
    break;
      
    default:
      SWC_msg.Command_12Sts = 0x00;
      SWC_msg.Command_13Sts = 0x00;
      SWC_msg.Command_15Sts = 0x00;
      SWC_msg.Command_14Sts = 0x00;
      SWC_msg.Command_11Sts = 0x00;
      SWC_msg.Command_10Sts = 0x00;
    break;
  }
  //output(SWC_msg);
}


on sysvar sysvar::RoadMap::BCM_VPS2_ACTIVE
{
  if(0 == sysGetVariableInt(sysvar::RoadMap::BCM_VPS2_ACTIVE))
  {
    cancelTimer(BCM_VPS2_Timer);
  }
  else if(1 == sysGetVariableInt(sysvar::RoadMap::BCM_VPS2_ACTIVE) && 1 == sysGetVariableInt(sysvar::RoadMap::WC_ON))
  {
    setTimer(BCM_VPS2_Timer, BCM_VPS2_Cycle);
  }
}


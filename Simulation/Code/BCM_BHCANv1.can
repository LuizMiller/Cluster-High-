  case 0x1:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x1; 
    SysSetVariableInt(sysvar::RoadMap::KL15,0);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,0);
    b1=0;
    b2=0;
  break;

  case 0x2:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x2; 
    SysSetVariableInt(sysvar::RoadMap::KL15,0);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,0);
    b1=0;
    b2=0;
  break;

  case 0x3:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x3; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
    b2=0;
  break;

  case 0x4:
    sysSetVariableInt(sysvar::RoadMap::NETWORK_CCAN_Status,1);
    sysSetVariableInt(sysvar::RoadMap::NETWORK_BHCAN_Status,1);
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x4;
    //sysSetVariableInt(sysvar::RoadMap::CmdIgnSts,4); 
    b2=0;
    //sysSetVariableInt(sysvar::RoadMap::EngineSts,0);
      
    sysSetVariableInt(sysvar::RoadMap::SystemCommand, 0);
    NWM_BCM_msg.SystemCommand = BHCAN::BCM::NWM_BCM.SystemCommand::Wake_up;
    output(NWM_BCM_msg);
    // write("\n 3 ");
    setTimer(System_command_wakeup_timer, System_command_wakeup_cycle);
  break;

  case 0x5:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x5;
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0x6:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x6;
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0x7:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x7; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0x8:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x8;
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
    //sysSetVariableInt(sysvar::RoadMap::CmdIgnSts,5);
  break;

  case 0x9:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0x9; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0xA:
    SysSetVariableInt(sysvar::RoadMap::KL15,0);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,0);
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xA;                 
    //sysSetVariableInt(sysvar::RoadMap::CmdIgnSts,1);
    b2=0;
  break;

  case 0xB:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xB; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0xC:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xC; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0xD:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xD; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
    SysSetVariableInt(sysvar::RoadMap::WC_ON,1);
  break;

  case 0xE:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xE; 
    SysSetVariableInt(sysvar::RoadMap::KL15,1);
  break;

  case 0xF:
    STATUS_BH_BCM1_msg.OperationalModeSts = 0xF; 
  break;

  default:
  break;
  }
}

on timer sleep
{
  int i = 0;
  i = i+1;    
  if(i == sysgetVariableInt(sysvar::RoadMap::SleepTime) && sysgetVariableInt(sysvar::RoadMap::WC_ON) == 0)
  {
    i=0; 
    cancelTimer(BCM_VPS2_Timer);
    cancelTimer(EXTERNAL_LIGHTS_Timer);
    cancelTimer(HAPTIC1_Timer);
    cancelTimer(HAPTIC2_Timer);
    cancelTimer(NWM_BCM_Timer);
    cancelTimer(PARK_INFO_Timer);
    cancelTimer(SDW_INFO_Timer);
    cancelTimer(STATUS_BH_BCM1_Timer);
    cancelTimer(STATUS_BH_BCM2_Timer);
    cancelTimer(STATUS_BH_BCM4_Timer); //added V0011
    cancelTimer(STATUS_CCAN8_Timer); //added V0011
    cancelTimer(STATUS_PAM_Timer);
    cancelTimer(SWC_Timer);
    cancelTimer(VIN_Timer);
    // write("entrou em sleep");
    cancelTimer(sleep);
    
    if(sysGetVariableInt(sysvar::RoadMap::NWM_BCM_ACTIVE) == 1)
    {
      sysSetVariableInt(sysvar::RoadMap::SystemCommand, 3);
      NWM_BCM_msg.SystemCommand = BHCAN::BCM::NWM_BCM.SystemCommand::Sleep;
      output(NWM_BCM_msg);
      // write("\n 4 ");
    }
    
    
  }
  else if (sysgetVariableInt(sysvar::RoadMap::WC_ON)==1 || sysgetVariableInt(sysvar::RoadMap::OperationalModeSts)>= 4 
          || sysgetVariableInt(sysvar::RoadMap::NETWORK_BHCAN_Status)==1)
//  else if (sysgetVariableInt(sysvar::RoadMap::WC_ON)==1 || sysgetVariableInt(sysvar::RoadMap::OperationalModeSts)>= 4)
  {
    i=0; 
    cancelTimer(sleep);
  }
}

on Timer wakeup_sleep_time
{
  int i = 0;
  i = i+1;
  if(i == 1)
  {
     sysSetVariableInt(sysvar::RoadMap::NETWORK_BHCAN_Status,0);
     sysSetVariableInt(sysvar::RoadMap::NETWORK_CCAN_Status,0);
     i=0;
     canceltimer(wakeup_sleep_time);
  }
}


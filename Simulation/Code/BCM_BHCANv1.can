{
  SWC_msg.Command_11Sts = sysGetVariableInt(sysvar::RoadMap::Command_11Sts);
    if(sysGetVariableInt(sysvar::RoadMap:: SWC_ACTIVE) == 1)
    {
      output(SWC_msg);

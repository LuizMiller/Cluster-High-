on sysvar sysvar::RoadMap::WC_ON
{
  if(sysGetVariableInt(sysvar::RoadMap::WC_ON) == 1)
  {
    cancelTimer(sleep);
  }
  else
  {
    if(sysGetVariableInt(sysvar::RoadMap::SleepEnabled) == 1)
    {
      setTimerCyclic(sleep,1000);
    }
  }
}


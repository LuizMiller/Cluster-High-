on sysvar sysvar::RoadMap::VINString
{
  int i;
  for(i = 0;i <= 20; i++)
  {
    VIN_buffer[i] = 0x00;
  }
  sysGetVariableString(sysvar::RoadMap::VINString,VIN_buffer,255);
  
  for(i = 0;i <= 20; i++)
  {
    if(0 <= i && i <= 6)
    {
      VIN_LO[i] = VIN_buffer[i];
    }
    else if(7 <= i && i <= 13)
    {
      VIN_MID[i-7] = VIN_buffer[i];
    } 
    else if ( 14 <= i && i <= 16)
    {
      VIN_HI[i-14] = VIN_buffer[i];
    }
  }  
}


on sysvar sysvar::RoadMap::Command_10Sts
{
  SWC_msg.Command_10Sts = sysGetVariableInt(sysvar::RoadMap::Command_10Sts);
  if(sysGetVariableInt(sysvar::RoadMap:: SWC_ACTIVE) == 1)
  {
    output(SWC_msg);
  }
    //SWC_msg.Command_10Sts = 0x01;
    //Output(SWC_msg);
    setTimerCyclic(THome,delaybutton);
}

on key 'h'
{
    sysSetVariableInt(sysvar::RoadMap::Command_10Sts,1);
}

on timer THome
{
  int i =0;

  if(keyPressed())
  {
    SWC_msg.Command_10Sts = 0x01;
    //Output(SWC_msg);
  }
  else
  {
    if(i == 1)
    { 
      SWC_msg.Command_10Sts = 0x00;
      output(SWC_msg);
      sysSetVariableInt(sysvar::RoadMap::Command_10Sts,0);
      canceltimer(THome);
      i=0;
    }
    i=i+1;
  }
}

on sysvar sysvar::RoadMap::HomeKeyStuck
{
  if(1 == sysGetVariableInt(sysvar::RoadMap::HomeKeyStuck))
  {
    SWC_msg.Command_10Sts = 0x01;
    Output(SWC_msg);
//    button = home;
//    setTimerCyclic(stuckButton,10);
  }
  else if(0 == sysGetVariableInt(sysvar::RoadMap::HomeKeyStuck))
  {
//    CancelTimer(stuckButton);
    SWC_msg.Command_10Sts = 0x00;
    Output(SWC_msg);
  }
}


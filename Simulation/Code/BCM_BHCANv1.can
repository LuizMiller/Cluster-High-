on key Cursorleft
{
    sysSetVariableInt(sysvar::RoadMap::Command_11Sts,1);
}

on timer TLeft
{
  int i =0;
  
  if(keyPressed())
  {
    SWC_msg.Command_11Sts = 0x01;
    //Output(SWC_msg);
  }
  else
  {
    if(i == 1)
    { 

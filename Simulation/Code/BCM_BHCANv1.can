on sysvar sysvar::RoadMap::VIN_ACTIVE_BH_CAN
{
  if(0 == sysGetVariableInt(sysvar::RoadMap::VIN_ACTIVE_BH_CAN))
  {
    cancelTimer(VIN_Timer);
  }
  else if(1 == sysGetVariableInt(sysvar::RoadMap::VIN_ACTIVE_BH_CAN) && 1 == sysGetVariableInt(sysvar::RoadMap::WC_ON))
  {
    setTimer(VIN_Timer, VIN_Cycle);
  }
}

on sysvar sysvar::RoadMap::VIN_DLC_BH_CAN
{
  if(sysGetVariableInt(sysvar::RoadMap::VIN_DLC_BH_CAN) == 8)
  {
    VIN_msg.dlc = VIN.dlc;
  }
  else
  {
    VIN_msg.dlc = sysGetVariableInt(sysvar::RoadMap::VIN_DLC_BH_CAN);
  }
}

